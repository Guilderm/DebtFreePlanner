{
  "name": "debtfreeplanner-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "sonar": "sonar-scanner",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx,vue}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx,js,jsx,vue}\"",
    "test": "vitest",
    "cleanup": "npm run format && npm run lint:fix && npm run sonar",
    "backup": "npm run cleanup && powershell -ExecutionPolicy Bypass -File ../Utilities/Backup/Repository_CleanAndBackup.ps1"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "pinia": "^2.2.4",
    "primeicons": "^7.0.0",
    "primevue": "^4.1.0",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@pinia/testing": "^0.1.6",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-typescript": "^14.0.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "prettier": "^3.3.3",
    "sonarqube-scanner": "^4.2.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.8.1",
    "vite": "^5.4.8",
    "vite-plugin-eslint": "^1.8.1",
    "vitest": "^2.1.2",
    "vue-tsc": "^2.1.6"
  },
  "eslintConfig": {}
}
